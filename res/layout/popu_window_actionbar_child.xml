<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#00AEAE"
    android:orientation="vertical" >
  
    <TextView 
        android:id="@+id/tv_popu_window_actionbar_child_about"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_popu_window_actionbar_child_item_background"
        android:clickable="true"
        android:textSize="20sp"
        android:textColor="#000000"
        android:text="关于"/>
    <TextView 
        android:id="@+id/tv_popu_window_actionbar_child_register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_popu_window_actionbar_child_item_background"
        android:clickable="true"
        android:textSize="20sp"
        android:textColor="#000000"
        android:text="登陆"/>
    <TextView 
        android:id="@+id/tv_popu_window_actionbar_child_exit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_popu_window_actionbar_child_item_background"
        android:clickable="true"
        android:textSize="20sp"
        android:textColor="#000000"
        android:text="退出"/>

</LinearLayout>
